	Language = {English},
	Number = {423},
	Pages = {1149--1159},
	Publisher = {American Statistical Association},
	Title = {Hierarchical Logistic Regression Models for Imputation of Unresolved Enumeration Status in Undercount Estimation},
	Url = {http://www.jstor.org/stable/2290812},
	Volume = {88},
	Year = {1993},
