	Language = {English},
	Number = {2},
	Pages = {pp. 174-199},
	Publisher = {University of California Press on behalf of the Society for the Study of Social Problems},
	Title = {Respondent-Driven Sampling: A New Approach to the Study of Hidden Populations},
	Url = {http://www.jstor.org/stable/3096941},
	Volume = {44},
	Year = {1997},
	Bdsk-Url-1 = {http://www.jstor.org/stable/3096941}}

